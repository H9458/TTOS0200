        <TextBox x:Name="txbCar" HorizontalAlignment="Left" Height="23" Margin="319,216,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" TextChanged="txbCar_TextChanged"/>
        <TextBox x:Name="txbTruck" HorizontalAlignment="Left" Height="23" Margin="81,216,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" TextChanged="txbTruck_TextChanged"/>

    </Grid>
